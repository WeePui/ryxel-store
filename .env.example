# Public API base URL (used on the client)
NEXT_PUBLIC_API_URL=http://your-api-url

# Internal API base URL (used on the server)
API_URL=http://your-api-url/api/v1

# JWT cookie expiration (in days)
JWT_COOKIES_EXPIRES_IN=30

# Reauthenticated expiration (in minutes) - used for reauthentication flows
REAUTHENTICATED_COOKIES_EXPIRES_IN=30

# Limit stock threshold for displaying stock information
NEXT_PUBLIC_STOCK_LIMIT=0
